{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next Develop",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/next",
      "args": ["dev"],
      "runtimeArgs": ["--nolazy"],
